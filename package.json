{
  "name": "ngx-inject",
  "version": "2.0.0",
  "description": "Dependency injection utilities for Angular",
  "homepage": "https://github.com/dscheerens/ngx-inject",
  "author": "Daan Scheerens",
  "license": "MIT",
  "keywords": [
    "angular",
    "ngx",
    "inject",
    "dependency",
    "injection",
    "di",
    "provider",
    "utils",
    "utilities"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/dscheerens/ngx-inject.git"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve --project=demo",
    "serve.prod": "ng serve --project=demo --prod",
    "lint": "tslint -p lib/tsconfig.lint.json -t stylish -c tslint.json",
    "test": "ng test --project=lib --watch=false --code-coverage",
    "test.watch": "ng test --project=lib --watch=true --code-coverage",
    "build": "ng build --project=lib --prod",
    "build.demo": "ng build --project=demo --prod",
    "release": "standard-version --message \"release: v%s\"",
    "publish.dist": "npm publish build/dist"
  },
  "sideEffects": false,
  "peerDependencies": {
    "@angular/core": ">=8.0.0",
    "tslib": "^1.9.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.21",
    "@angular-devkit/build-ng-packagr": "~0.803.21",
    "@angular/cli": "^8.3.21",
    "@angular/common": "^8.2.14",
    "@angular/compiler": "^8.2.14",
    "@angular/compiler-cli": "^8.2.14",
    "@angular/core": "^8.2.14",
    "@angular/platform-browser": "^8.2.14",
    "@angular/platform-browser-dynamic": "^8.2.14",
    "@dscheerens/tslint-presets": "^6.0.0",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.8",
    "ci-info": "^2.0.0",
    "codelyzer": "^5.1.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "^3.0.0",
    "karma-coverage-istanbul-reporter": "^2.1.0",
    "karma-jasmine": "^2.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "ng-packagr": "^5.7.1",
    "rxjs": "^6.5.4",
    "standard-version": "8.0.1",
    "tsickle": "0.37.0",
    "tslib": "^1.10.0",
    "tslint": "5.18.0",
    "typescript": "~3.5.3",
    "typescript-tslint-plugin": "^0.5.5",
    "zone.js": "~0.9.1"
  }
}
